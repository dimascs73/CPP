#include <iostream>

class Tiles
{

public:
    char brand = '0';
    double size_h = 0.0;
    double size_w = 0.0;
    int price = 0;

    Tiles (char(brand), double(size_h), double(size_w), int(price))
        :brand(brand), size_h(size_h), size_w(size_w)
        ,price(price) {}
    
    template <typename T>
    Type getData()
    {
      return Type;
    }
};


int main ()
{
    Tiles t1 {"brand1", 3.5, 2.5, 500};
    Tiles t2 {"brand2", 4.5, 1.5, 200};

    t1.getData();
    
    return 0;
}